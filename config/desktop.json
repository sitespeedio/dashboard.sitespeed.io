{
  "extends": "/config/secrets.json",
  "browsertime": {
    "connectivity": {
      "engine": "throttle",
      "profile": "cable"
    },
    "visualElements": true,
    "visualMetricsNotification": true,
    "iterations": 3,
    "browser": "chrome",
    "video": true,
    "visualMetric": true,
    "visualMetricsPortable": true,
    "videoParams": {
      "filmstripFullSize": true
    },
    "chrome": {
      "collectConsoleLog": true,
      "enableTraceScreenshots": true,
      "args": [
        "disable-features=PaintHoldingCrossOrigin"
      ]
    },
    "firefox": {
      "includeResponseBodies": "all",
      "disableBrowsertimeExtension": true,
      "geckoProfiler": true
     }
  },
  "cpu": true,
  "gzipHAR": true,
  "html": {
    "fetchHARFiles": true,
    "showScript": true,
    "compareURL": "https://compare.sitespeed.io/"
  },
  "screenshot": {
    "type": "jpg"
  },
  "slug": "firstView",
  "copyLatestFilesToBase": true,
  "graphite": {
    "namespace": "sitespeed_io.desktop",
    "addSlugToKey": true
  },
  "thirdParty": {
    "cpu": true
  }
}
